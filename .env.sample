# Database Connection
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=myuser
DB_PASSWORD=mypassword
DB_NAME=mydb

# Logging
LOG_LEVEL=INFO
LOG_FILE=app.log

# Application Configuration
APP_ENV=production
APP_NAME=myapp
APP_VERSION=1.0.0
APP_TIMEZONE=Africa/Nairobi

# External Services
API_KEY=my_api_key
SECRET_KEY=my_secret_key

# Security
SESSION_SECRET=a_very_secret_key
CSRF_TOKEN=random_token

# Performance
CACHE_ENABLED=true
CACHE_EXPIRE=3600
MAX_REQUEST_SIZE=10MB